{
  "name": "11ty-vscode-snippet-generator",
  "version": "1.0.0",
  "description": "11ty vscode snippet generator",
  "main": "index.js",
  "scripts": {
    "build": "npx @11ty/eleventy && npm run prettier",
    "debug": "DEBUG=Eleventy* npx @11ty/eleventy --watch",
    "dev": "npx @11ty/eleventy --watch",
    "prettier": "npx prettier --write dist/**/*.json",
    "start": "npx @11ty/eleventy --serve"
  },
  "keywords": [
    "11ty",
    "njk",
    "nunjucks",
    "snippet-generator",
    "snippets",
    "visual studio code",
    "vscode-snippets",
    "vscode"
  ],
  "author": "James Deane",
  "license": "MIT",
  "devDependencies": {
    "@11ty/eleventy": "^1.0.0",
    "prettier": "^2.5.1"
  }
}
